PREFIX doapk: <https://w3id.org/doap-kg#>
PREFIX prov:  <http://www.w3.org/ns/prov#>
SELECT ?tx ?activity ?agent ?evidence WHERE {
  VALUES ?tx { doapk:TX_4721 }
  ?tx (doapk:hasControlExecution|doapk:hasRiskAssessment|doapk:hasOverride) ?activity .
  OPTIONAL { ?activity prov:wasAssociatedWith ?agent . }
  OPTIONAL { ?activity doapk:basedOnEvidence ?evidence . }
}
